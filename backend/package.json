{
  "name": "my-wedding-backend",
  "version": "1.0.0",
  "private": true,
  "description": "wedding backend node",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "npm run test",
    "lint": "eslint ."
  },
  "keywords": [
    "node",
    "eslint",
    "express",
    "googleapis"
  ],
  "author": "Sara Jimenez & Alberto Mardomingo",
  "license": "MIT",
  "dependencies": {
    "express": "4.19.2",
    "googleapis": "134.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.7.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "globals": "15.8.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2020": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "ignorePatterns": [
      "/node_modules/"
    ],
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "import/extensions": [
        "off",
        "always"
      ]
    }
  }
